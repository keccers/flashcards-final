  <table>
    <% if @posts.any? %>
    <thead>
      <th>Title</th>
      <th>Created</th>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
      <tr>
        <td><a href=#####################><%#= ############### %></a></td>
        <td><%= post.created_at.strftime("Created at:%m/%y") %></td>
      </tr>
    </tbody>
  </table>
  <%# end %>
  <%# else %>
  <div><h2>No posts to display.</h2></div> 
  <%# end %>
